using System;
					
public class Program
{
	public static void Main()
        {
            {
            double C, F;
            Console.Write("Digite a temperatura em Fahrenheit: ");
            F = double.Parse(Console.ReadLine());
            C = ((F - 32)*5) / 9;
            Console.WriteLine("A temperatura em Celsius é: ºC {0}. ", C);
}
}
}
