using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Repetição_até_100
{
    class Program
    {
        static void Main(string[] args)
        {
            int i;
            for (i = 1; i < 101 / 2; i++)
                Console.WriteLine("Número par " + i * 2);
        }
    }
}
