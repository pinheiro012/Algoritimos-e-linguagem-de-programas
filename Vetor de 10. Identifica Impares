using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] v = new int[10];
            for (int i = 0; i < 9; i++)
            {
                v[i] = int.Parse(Console.ReadLine());
            }
            {
                Console.WriteLine("Numeros impares: ");
                {

                    for (int i = 9; i >= 0; i--)
                    {
                        if(v[i]%2==1)
                        Console.WriteLine(v[i]);
                    }
                }

            }
        }
    }
}

