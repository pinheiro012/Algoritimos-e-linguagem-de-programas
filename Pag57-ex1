using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Projeto03
{
    class Program
    {
        static void Main(string[] args)
        {
            double num1, num2, mult;
            num1 = double.Parse(Console.ReadLine());
            num2 = double.Parse(Console.ReadLine());
            mult = num1 * num2;
            Console.WriteLine(mult);
        }
    }
}
