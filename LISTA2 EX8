int media = 0,soma = 0;
            int[] v = new int[15];
            Console.WriteLine("Digite as notas: ");
            for (int i = 0; i< 15; i++)
            {
                
                v[i] = Convert.ToInt32(Console.ReadLine());

            }
            for (int i = 0; i< 15; i++)
            {
                soma += v[i];
            }
            media = soma / v.Length;

            Console.WriteLine(media);

            
        }

