using System;
					
public class Program
{
	public static void Main()
        {
            {
            double x;
            Console.Write("Digite um número: ");
            x = double.Parse(Console.ReadLine());
            if (x >= 100 && x < 201)
            {
                Console.WriteLine("{0} está entre o intervalo de 100 e 200.", x);
            }
            else
            {
                Console.WriteLine("{0} não está entre o intervalo de 100 e 200.", x);
            }
}
}
}
